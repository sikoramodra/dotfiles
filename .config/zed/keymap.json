[
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-`": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "vim_mode == normal && !menu",
    "bindings": {
      "ctrl-e": "file_finder::Toggle"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-`": "workspace::ToggleBottomDock",
      "ctrl-t": "workspace::NewTerminal",
      "ctrl-w": "pane::CloseActiveItem"
    }
  }
]
